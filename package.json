{
  "name": "vue-hackernews-2.0",
  "description": "A Vue.js project",
  "private": true,
  "scripts": {
    "start": "node server",
    "dev": "cross-env NODE_ENV=development node server",
    "dev:spa": "cross-env NODE_ENV=development && node config/development.js",
    "build:spa": "rimraf html && cross-env NODE_ENV=production node config/build.js",
    "build-server": "webpack --config server/config.js --progress --hide-modules",
    "pc-cpts": "cross-env PRO_ENV=pc_components node config/development.js",
    "mobile-example": "cross-env PRO_ENV=tk_components/example node config/development.js",
    "mobile-example-build": "cross-env PRO_ENV=tk_components/example node config/build.js",
    "start-server": "node server/server.js",
    "dev-server": "webpack --config server/config.js --watch &",
    "build": "rimraf dist && npm run build:client && npm run build:server",
    "build:client": "cross-env NODE_ENV=production webpack --config config/webpack.client.config.js --progress --hide-modules",
    "build:server": "cross-env NODE_ENV=production webpack --config config/webpack.server.config.js --progress --hide-modules"
  },
